      <default>7</default>
      <locale name="C">
        <short></short>
        <long></long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/gnome_settings_daemon/plugins/touchpad/active</key>
      <applyto>/apps/gnome_settings_daemon/plugins/touchpad/active</applyto>
