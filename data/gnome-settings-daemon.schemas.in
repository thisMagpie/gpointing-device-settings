<?xml version="1.0"?>
<gconfschemafile>
  <schemalist>

    <schema>
      <key>/schemas/apps/gnome_settings_daemon/plugins/trackpoint/active</key>
      <applyto>/apps/gnome_settings_daemon/plugins/trackpoint/active</applyto>
      <owner>gnome-settings-daemon</owner>
      <type>bool</type>
      <default>TRUE</default>
      <locale name="C">
        <short>Enable trackpoint plugin</short>
        <long>Set to True to enable the plugin to manage trackpoint settings.</long>
